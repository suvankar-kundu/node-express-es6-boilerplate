{
  "name": "node-express-es6-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Node.js projects with lint, transpile and test configurations.",
  "main": "src/index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --config ./build/webpack.api.base.babel.js",
    "clean": "./node_modules/.bin/rimraf ./dist",
    "lint": "./node_modules/.bin/eslint --color --format stylish src",
    "prebuild": "npm run clean",
    "start": "./node_modules/.bin/babel-node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Suvankar Kundu",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.17.0",
    "eslint-plugin-import": "^2.8.0",
    "file-loader": "^1.1.6",
    "json-loader": "^0.5.7",
    "rimraf": "^2.6.2",
    "swagger-ui-express": "^4.0.2",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "express": "4.16.2",
    "mongoose": "5.0.9",
    "morgan": "1.9.0",
    "passport": "0.4.0",
    "passport-jwt": "3.0.1",
    "winston": "2.4.0",
    "winston-daily-rotate-file": "1.7.2"
  }
}
